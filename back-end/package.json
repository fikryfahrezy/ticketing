{
  "name": "back-end",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun src/index.ts",
    "lint": "eslint src",
    "migrate:build": "esbuild migrate.mjs --platform=node --bundle --format=esm --outfile=dist/migrate.mjs",
    "migrate:up": "bun --env-file=./.env run ./migrate.mjs",
    "swag:gen": "bun run swagger.mjs",
    "type-check": "tsc --noEmit",
    "prepare": "if [ \"$NODE_ENV\" != 'production' -a \"$CI\" != 'true' ]; then cd .. && husky; fi"
  },
  "devDependencies": {
    "@eslint/js": "10.0.1",
    "@types/bun": "1.3.8",
    "@types/swagger-ui-express": "4.1.8",
    "esbuild": "0.27.3",
    "eslint": "10.0.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "postgres-shift": "0.1.0",
    "prettier": "3.8.1",
    "swagger-autogen": "2.23.7",
    "typescript-eslint": "8.54.0"
  },
  "peerDependencies": {
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@types/express": "5.0.6",
    "express": "5.2.1",
    "openai": "6.21.0",
    "postgres": "3.4.8",
    "swagger-ui-express": "5.0.1"
  }
}
